{
	"name": "file-insights",
	"displayName": "File Insights",
	"description": "A simple extension to display file information",
	"version": "0.0.1",
	"license": "MIT",
	"publisher": "VijayGangatharan",
	"repository": {
		"type": "git",
		"url": "https://github.com/Vijay431/vscode_file-insights_extension.git"
	},
	"bugs": {
		"url": "https://github.com/Vijay431/vscode_file-insights_extension/issues"
	},
	"engines": {
		"vscode": "^1.90.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:file-insights.activate",
		"onCommand:file-insights.deactivate",
		"onStartupFinished"
	],
	"contributes": {
		"commands": [
			{
				"command": "file-insights.activate",
				"title": "File insights: Activate"
			},
			{
				"command": "file-insights.deactivate",
				"title": "File insights: Deactivate"
			}
		]
	},
	"main": "./dist/extension.js",
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint",
		"watch": "tsc -watch -p ./",
		"package": "vsce package",
		"publish": "vsce publish"
	},
	"devDependencies": {
		"@eslint/js": "^9.13.0",
		"@stylistic/eslint-plugin": "^2.9.0",
		"@types/fs-extra": "^11.0.4",
		"@types/vscode": "^1.73.0",
		"eslint": "^9.13.0",
		"typescript": "^5.7.2",
		"typescript-eslint": "^8.16.0"
	},
	"dependencies": {
		"fs-extra": "^11.2.0"
	}
}
